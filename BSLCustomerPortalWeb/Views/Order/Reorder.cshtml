<section id="contact-page" class="pt-10 pb-10 gray-bg">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="shop-top-search">
                    <div class="shop-bar">
                        <ul class="nav" id="myTab" role="tablist">
                            <li class="nav-item">
                                <a href="@Url.Action("DashBoard", "Home")"><i class="fa fa-home"></i></a> /
                            </li>
                            <li class="nav-item">
                                Reorder /
                            </li>
                            <li class="nav-item" id="liprm"> </li>
                        </ul>
                    </div>
                    <div class="shop-select">
                        <a href="@Url.Action("CreateReorder", "Order")" class="btn btn-outline-success" >Create Reorder</a>
                    </div>
                </div>
            </div>
        </div> <!-- row -->

    </div> <!-- container -->
